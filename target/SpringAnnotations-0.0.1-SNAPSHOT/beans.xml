<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<context:annotation-config></context:annotation-config>
<bean id="add" class="com.jfs.spring.SpringAnnotations.Address">
<property name="street" value="Tpt"/>
<property name="city" value="Chennai"/>
<property name="country" value="India"/>
</bean>
<bean id="add1" class="com.jfs.spring.SpringAnnotations.Address">
<property name="street" value="Thimmireddypalli"/>
<property name="city" value="Chittoor"/>
<property name="country" value="India"/>
</bean>

    
<bean id="emp" class="com.jfs.spring.SpringAnnotations.Employee" >

</bean> 
</beans>